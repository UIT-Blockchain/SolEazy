{
  "name": "guzer",
  "private": true,
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts,.json}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts,.json}\" --check",
    "app:dev": "next dev",
    "app:build": "yarn anchor:test && next build",
    "app:start": "next start",
