    "@solana/wallet-adapter-wallets": "^0.19.7",
    "@solana/web3.js": "^1.69.0",
    "@tailwindcss/forms": "^0.5.0",
    "animate.css": "^4.1.1",
    "axios": "^1.2.1",
    "bignumber.js": "^9.0.2",
    "bs58": "^5.0.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
